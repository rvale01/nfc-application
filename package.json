{
  "name": "monorepo-app-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --continue",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "*",
    "turbo": "*",
    "@babel/core": "^7.12.9"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "react": "18.2.0"
  },
  "packageManager": "yarn@1.22.10"
}
